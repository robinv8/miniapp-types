{
  "title": "channel-video",
  "type": "object",
  "properties": {
    "feed-id": {
      "type": "string",
      "description": "视频 feedId"
    },
    "finder-user-name": {
      "type": "string",
      "description": "视频号 id，以“sph”开头的id，可在视频号助手获取。视频号必须与当前小程序相同主体。"
    },
    "autoplay": {
      "type": "boolean",
      "description": "是否自动播放",
      "defaultValue": false
    },
    "loop": {
      "type": "boolean",
      "description": "是否循环播放",
      "defaultValue": false
    },
    "muted": {
      "type": "boolean",
      "description": "是否静音播放",
      "defaultValue": false
    },
    "object-fit": {
      "type": "string",
      "description": "当视频大小与 video 容器大小不一致时，视频的表现形式",
      "defaultValue": "contain",
      "enum": [
        "contain",
        "fill",
        "cover"
      ]
    },
    "bindError": {
      "tsType": "() => void",
      "description": "视频播放出错时触发"
    }
  },
  "additionalProperties": false,
  "required": [
    "feed-id",
    "finder-user-name"
  ]
}