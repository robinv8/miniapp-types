{
  "title": "slider",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "组件名字，用于表单提交获取数据。"
    },
    "min": {
      "type": "number",
      "description": "最小值。",
      "defaultValue": 0
    },
    "max": {
      "type": "number",
      "description": "最大值。",
      "defaultValue": 100
    },
    "step": {
      "type": "number",
      "description": "步长，值必须大于 0，并可被（max-min）整除。",
      "defaultValue": 1
    },
    "disabled": {
      "type": "boolean",
      "description": "是否禁用。",
      "defaultValue": false
    },
    "value": {
      "type": "number",
      "description": "当前取值。",
      "defaultValue": 0
    },
    "show-value": {
      "type": "boolean",
      "description": "是否显示当前 value。",
      "defaultValue": false
    },
    "active-color": {
      "type": "string",
      "description": "已选择的颜色，同 CSS 色值。",
      "defaultValue": "#108ee9"
    },
    "background-color": {
      "type": "string",
      "description": "背景条颜色，同 CSS 色值。",
      "defaultValue": "#ddd"
    },
    "track-size": {
      "type": "number",
      "description": "轨道线条高度。",
      "defaultValue": 4
    },
    "handle-size": {
      "type": "number",
      "description": "滑块大小。",
      "defaultValue": 22
    },
    "handle-color": {
      "type": "string",
      "description": "滑块填充色，同 CSS 色值。",
      "defaultValue": "#fff"
    },
    "onChange": {
      "tsType": "() => void",
      "description": "完成一次拖动后触发，event.detail = {value: value}。"
    },
    "onChanging": {
      "tsType": "() => void",
      "description": "拖动过程中触发的事件，event.detail = {value: value}。"
    }
  },
  "additionalProperties": false
}