  {
    "name": "stroke-width",
    "type": [
      "Number",
      "String"
    ],
    "defaultValue": "2",
    "description": "进度条的高度（单位：px）",
    "required": false
  },
  {
    "name": "border-radius",
    "type": [
      "Number",
      "String"
    ],
    "defaultValue": "0",
    "description": "圆角大小（单位：px）",
    "required": false
  },
  {
    "name": "font-size",
    "type": [
      "Number",
      "String"
    ],
    "defaultValue": "16",
