[
  {
    "name": "url",
    "type": "string",
    "description": "推流地址。目前仅支持 `rtmp` 格式",
    "required": false
  },
  {
    "name": "mode",
    "type": "string",
    "defaultValue": "RTC",
    "description": "`SD`（标清）, `HD`（高清）, `FHD`（超清）, `RTC`（实时通话）",
    "required": false
  },
  {
    "name": "autopush",
    "type": "boolean",
    "defaultValue": "false",
    "description": "自动推流",
    "required": false
  },
  {
    "name": "muted",
    "type": "boolean",
    "defaultValue": "false",
    "description": "是否静音。即将废弃，可用 `enable-mic` 替代",
    "required": false
  },
  {
    "name": "enable-camera",
    "type": "boolean",
    "defaultValue": "true",
    "description": "开启摄像头",
    "required": false
  },
  {
    "name": "auto-focus",
    "type": "boolean",
    "defaultValue": "true",
    "description": "自动聚集",
    "required": false
  },
  {
    "name": "orientation",
    "type": "string",
    "options": [
      "vertical",
      "horizontal"
    ],
    "defaultValue": "vertical",
    "description": "画面方向",
    "required": false
  },
  {
    "name": "beauty",
    "type": "number",
    "defaultValue": "0",
    "description": "美颜，取值范围 0-9 ，0 表示关闭",
    "required": false
  },
  {
    "name": "whiteness",
    "type": "number",
    "defaultValue": "0",
    "description": "美白，取值范围 0-9 ，0 表示关闭",
    "required": false
  },
  {
    "name": "aspect",
    "type": "string",
    "defaultValue": "9:16",
    "description": "宽高比，可选值有 `3:4`, `9:16`",
    "required": false
  },
  {
    "name": "min-bitrate",
    "type": "number",
    "defaultValue": "200",
    "description": "最小码率",
    "required": false
  },
  {
    "name": "max-bitrate",
    "type": "number",
    "defaultValue": "1000",
    "description": "最大码率",
    "required": false
  },
  {
    "name": "audio-quality",
    "type": "string",
    "defaultValue": "high",
    "description": "高音质(48KHz)或低音质(16KHz)，值为`high`, `low`",
    "required": false
  },
  {
    "name": "waiting-image",
    "type": "string",
    "description": "进入后台时推流的等待画面",
    "required": false
  },
  {
    "name": "waiting-image-hash",
    "type": "string",
    "description": "等待画面资源的MD5值",
    "required": false
  },
  {
    "name": "zoom",
    "type": "boolean",
    "defaultValue": "false",
    "description": "调整焦距",
    "required": false
  },
  {
    "name": "device-position",
    "type": "string",
    "defaultValue": "front",
    "description": "前置或后置，值为`front`, `back`",
    "required": false
  },
  {
    "name": "background-mute",
    "type": "boolean",
    "defaultValue": "false",
    "description": "进入后台时是否静音（已废弃，默认退后台静音）",
    "required": false
  },
  {
    "name": "mirror",
    "type": "boolean",
    "defaultValue": "false",
    "description": "设置推流画面是否镜像，产生的效果在 live-player 反应到",
    "required": false
  },
  {
    "name": "remote-mirror",
    "type": "boolean",
    "defaultValue": "false",
    "description": "同 mirror 属性，后续 mirror 将废弃",
    "required": false
  },
  {
    "name": "local-mirror",
    "type": "string",
    "options": [
      "auto",
      "enable",
      "disable"
    ],
    "defaultValue": "auto",
    "description": "控制本地预览画面是否镜像",
    "required": false
  },
  {
    "name": "audio-reverb-type",
    "type": "number",
    "options": [
      "0",
      "1",
      "2",
      "3",
      "4",
      "5",
      "6",
      "7"
    ],
    "defaultValue": "0",
    "description": "音频混响类型",
    "required": false
  },
  {
    "name": "enable-mic",
    "type": "boolean",
    "defaultValue": "true",
    "description": "开启或关闭麦克风",
    "required": false
  },
  {
    "name": "enable-agc",
    "type": "boolean",
    "defaultValue": "false",
    "description": "是否开启音频自动增益",
    "required": false
  },
  {
    "name": "enable-ans",
    "type": "boolean",
    "defaultValue": "false",
    "description": "是否开启音频噪声抑制",
    "required": false
  },
  {
    "name": "audio-volume-type",
    "type": "string",
    "options": [
      "auto",
      "media",
      "voicecall"
    ],
    "defaultValue": "auto",
    "description": "音量类型",
    "required": false
  },
  {
    "name": "video-width",
    "type": "number",
    "defaultValue": "360",
    "description": "上推的视频流的分辨率宽度",
    "required": false
  },
  {
    "name": "video-height",
    "type": "number",
    "defaultValue": "640",
    "description": "上推的视频流的分辨率高度",
    "required": false
  },
  {
    "name": "beauty-style",
    "type": "string",
    "options": [
      "smooth",
      "nature"
    ],
    "defaultValue": "smooth",
    "description": "设置美颜类型",
    "required": false
  },
  {
    "name": "filter",
    "type": "string",
    "options": [
      "standard",
      "pink",
      "nostalgia",
      "blues",
      "romantic",
      "cool",
      "fresher",
      "solor",
      "aestheticism",
      "whitening",
      "cerisered"
    ],
    "defaultValue": "standard",
    "description": "设置色彩滤镜",
    "required": false
  },
  {
    "name": "bindstatechange",
    "type": "eventhandle",
    "description": "状态变化事件，detail = {code}",
    "required": false
  },
  {
    "name": "bindnetstatus",
    "type": "eventhandle",
    "description": "网络状态通知，detail = {info}",
    "required": false
  },
  {
    "name": "binderror",
    "type": "eventhandle",
    "description": "渲染错误事件，detail = {errMsg, errCode}",
    "required": false
  },
  {
    "name": "bindbgmstart",
    "type": "eventhandle",
    "description": "背景音开始播放时触发",
    "required": false
  },
  {
    "name": "bindbgmprogress",
    "type": "eventhandle",
    "description": "背景音进度变化时触发，detail = {progress, duration}",
    "required": false
  },
  {
    "name": "bindbgmcomplete",
    "type": "eventhandle",
    "description": "背景音播放完成时触发",
    "required": false
  },
  {
    "name": "bindaudiovolumenotify",
    "type": "eventhandle",
    "description": "返回麦克风采集的音量大小",
    "required": false
  }
]