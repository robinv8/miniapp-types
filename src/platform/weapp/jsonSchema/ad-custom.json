{
  "title": "ad-custom",
  "type": "object",
  "properties": {
    "unit-id": {
      "type": "string",
      "description": "广告单元id，可在小程序管理后台的流量主模块新建"
    },
    "ad-intervals": {
      "type": "number",
      "description": "广告自动刷新的间隔时间，单位为秒，参数值必须大于等于30（该参数不传入时 模板 广告不会自动刷新）"
    },
    "bindload": {
      "type": "eventhandle",
      "description": "广告加载成功的回调"
    },
    "binderror": {
      "type": "eventhandle",
      "description": "广告加载失败的回调，event.detail = {errCode: 1002}"
    }
  }
}