[
  {
    "name": "version",
    "type": "string",
    "options": [
      "develop",
      "trial",
      "release"
    ],
    "defaultValue": "release",
    "description": "跳转到的小程序版本，**线上版本必须设置为 release**",
    "required": false
  },
  {
    "name": "name",
    "type": "string",
    "options": [
      "loginAndGetUserInfo",
      "requestPayment",
      "chooseAddress",
      "chooseInvoice",
      "chooseInvoiceTitle"
    ],
    "description": "要跳转到的功能页",
    "required": false
  },
  {
    "name": "args",
    "type": "object",
    "description": "功能页参数，参数格式与具体功能页相关",
    "required": false
  },
  {
    "name": "bindsuccess",
    "type": "eventhandler",
    "description": "功能页返回，且操作成功时触发， detail 格式与具体功能页相关",
    "required": false
  },
  {
    "name": "bindfail",
    "type": "eventhandler",
    "description": "功能页返回，且操作失败时触发， detail 格式与具体功能页相关",
    "required": false
  },
  {
    "name": "bindcancel",
    "type": "eventhandler",
    "description": "因用户操作从功能页返回时触发",
    "required": false
  }
]