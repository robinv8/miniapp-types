{
  "title": "page-container",
  "type": "object",
  "properties": {
    "show": {
      "type": "boolean",
      "description": "是否显示容器组件"
    },
    "duration": {
      "type": "number",
      "description": "动画时长，单位毫秒"
    },
    "z-index": {
      "type": "number",
      "description": "z-index 层级"
    },
    "overlay": {
      "type": "boolean",
      "description": "是否显示遮罩层"
    },
    "position": {
      "type": "string",
      "description": "弹出位置，可选值为 top bottom right center"
    },
    "round": {
      "type": "boolean",
      "description": "是否显示圆角"
    },
    "close-on-slideDown": {
      "type": "boolean",
      "description": "是否在下滑一段距离后关闭"
    },
    "overlay-style": {
      "type": "string",
      "description": "自定义遮罩层样式"
    },
    "custom-style": {
      "type": "string",
      "description": "自定义弹出层样式"
    },
    "bind:beforeenter": {
      "type": "eventhandle",
      "description": "进入前触发"
    },
    "bind:enter": {
      "type": "eventhandle",
      "description": "进入中触发"
    },
    "bind:afterenter": {
      "type": "eventhandle",
      "description": "进入后触发"
    },
    "bind:beforeleave": {
      "type": "eventhandle",
      "description": "离开前触发"
    },
    "bind:leave": {
      "type": "eventhandle",
      "description": "离开中触发"
    },
    "bind:afterleave": {
      "type": "eventhandle",
      "description": "离开后触发"
    },
    "bind:clickoverlay": {
      "type": "eventhandle",
      "description": "点击遮罩层时触发"
    }
  }
}