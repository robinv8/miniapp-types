[
  {
    "name": "aweme-id",
    "type": "string",
    "description": "用户的抖音号",
    "required": true
  },
  {
    "name": "type",
    "type": "\"avatar\" | \"nickname\"",
    "options": [
      "avatar",
      "nickname"
    ],
    "defaultValue": "\"avatar\"",
    "description": "类型，可以选择头像或昵称",
    "required": false
  },
  {
    "name": "disable-default",
    "type": "boolean",
    "defaultValue": "false",
    "description": "禁用默认行为。点击头像时，如果用户处于直播状态下默认会跳转到直播间，非直播状态下跳转到个人主页。如果为 true，点击头像时不会有默认行为。",
    "required": false
  },
  {
    "name": "default-avatar",
    "type": "string",
    "description": "获取信息失败时显示的默认头像 url",
    "required": false
  },
  {
    "name": "default-text",
    "type": "string",
    "description": "获取信息失败时显示的默认昵称文本",
    "required": false
  },
  {
    "name": "binderror",
    "type": "EventHandle",
    "description": "当错误发生时触发",
    "required": false
  }
]