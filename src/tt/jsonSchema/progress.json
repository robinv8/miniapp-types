{
  "title": "progress",
  "type": "object",
  "properties": {
    "percent": {
      "type": "number",
      "description": "进度",
      "defaultValue": 0
    },
    "stroke-width": {
      "type": "number",
      "description": "进度条的垂直宽度",
      "defaultValue": 6
    },
    "active-color": {
      "type": "string",
      "description": "进度条颜色",
      "defaultValue": "#F85959"
    },
    "background-color": {
      "type": "string",
      "description": "未达到进度的颜色",
      "defaultValue": "#EBEBEB"
    },
    "active": {
      "type": "boolean",
      "description": "进度条动画",
      "defaultValue": false
    },
    "active-mode": {
      "type": "string",
      "description": "backwards：动画从头播\nforwards：动画从上次结束点接着播",
      "defaultValue": "backwards"
    }
  },
  "additionalProperties": false
}