{
  "title": "map",
  "type": "object",
  "properties": {
    "longitude": {
      "type": "number",
      "description": "中心经度"
    },
    "latitude": {
      "type": "number",
      "description": "中心纬度"
    },
    "scale": {
      "type": "number",
      "description": "缩放级别，取值范围为 3-19"
    },
    "markers": {
      "type": "marker[]",
      "description": "标记点"
    },
    "circles": {
      "type": "circle[]",
      "description": "圆"
    },
    "show-location": {
      "type": "boolean",
      "description": "是否显示当前位置"
    },
    "polyline": {
      "type": "polyline[]",
      "description": "路线数组"
    },
    "include-points": {
      "type": "lngLat[]",
      "description": "缩放视野以包含所有给定的坐标点；示例：[{latitude: 0, longitude: 0}]"
    },
    "bindtap": {
      "type": "eventHandle",
      "description": "点击地图时触发，e.detail = { latitude, longitude}"
    },
    "bindmarkertap": {
      "type": "eventHandle",
      "description": "点击标记点时触发，e.detail = {markerId, latitude, longitude}"
    },
    "bindcallouttap": {
      "type": "eventHandle",
      "description": "点击标记点对应的气泡时触发, e.detail = {markerId}"
    },
    "bindregionchange": {
      "type": "eventHandle",
      "description": "视野发生变化时触发"
    },
    "rotate": {
      "type": "number",
      "description": "顺时针旋转角度，范围 0 ~ 360，旋转原点为地图容器中心点"
    },
    "skew": {
      "type": "number",
      "description": "倾斜角度，范围 0 ~ 40 , 关于 z 轴的倾角"
    },
    "polygons": {
      "type": "polygon[]",
      "description": "多边形"
    },
    "show-compass": {
      "type": "boolean",
      "description": "显示指南针"
    },
    "enable-overlooking": {
      "type": "boolean",
      "description": "开启俯视手势支持，双指上下滑调整俯仰角"
    },
    "enable-rotate": {
      "type": "boolean",
      "description": "开启旋转手势支持，双指旋转调整旋转角度"
    },
    "min-scale": {
      "type": "number",
      "description": "最小缩放级别，取值范围为 3 - max-scale"
    },
    "max-scale": {
      "type": "number",
      "description": "最大缩放级别，取值范围为 min-scale - 19"
    },
    "enable-3D": {
      "type": "boolean",
      "description": "是否展示 3D 楼块，该属性 IOS 不支持"
    },
    "show-scale": {
      "type": "boolean",
      "description": "是否显示比例尺"
    },
    "enable-zoom": {
      "type": "boolean",
      "description": "是否支持缩放"
    },
    "enable-scroll": {
      "type": "boolean",
      "description": "是否支持拖动"
    },
    "enable-satellite": {
      "type": "boolean",
      "description": "是否开启卫星图"
    },
    "enable-traffic": {
      "type": "boolean",
      "description": "是否开启实时路况，在该属性为 true 时，enable- building 和 enable-3D 无效"
    },
    "enable-poi": {
      "type": "boolean",
      "description": "是否显示地名"
    },
    "enable-building": {
      "type": "boolean",
      "description": "是否展示建筑物"
    },
    "bindlabeltap": {
      "type": "eventHandle",
      "description": "点击 label 时触发，e.detail = {markerId}"
    },
    "bindupdated": {
      "type": "eventHandle",
      "description": "在地图渲染更新完成时触发"
    },
    "bindanchorpointtap": {
      "type": "eventHandle",
      "description": "点击定位标时触发，e.detail = {longitude, latitude}"
    }
  }
}